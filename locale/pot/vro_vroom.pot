msgstr ""

msgid "``location_id``"
msgstr ""

msgid "Positive unique identifier of the location of the job."
msgstr ""

msgid "``setup``"
msgstr ""

msgid "|interval|"
msgstr ""

msgid "|interval0|"
msgstr ""

